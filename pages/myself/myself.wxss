page{
   background:#f8f9fa;
 }

 .usercontainer{
   vertical-align: top;
   margin:20rpx 20rpx 0;
 }

 .userbox{
   display: flex;
   justify-content: space-between;
   align-items:center;
   padding: 20rpx;
   background:#ffffff;
   border-radius: 20rpx;
   width: 700rpx;
 }
 .pic{
   width: 130rpx;
   height:130rpx;
   border-radius:50%;
 }

 .usercenter{
   flex:1;
   padding-left: 20rpx;
 }

 .title{
   font-size: 35rpx;
   font-weight: 600;
   color: #333333;
 }
 .person_text{
   padding-top: 10rpx;
  font-size: 35rpx;
  font-weight: 600;
  color: #999999;
}

.setting{
  margin-top: 20rpx;
  display: flex;
  padding:40rpx;
  justify-content: flex-start;
  align-items:center;
  background: #ffffff;
  border-radius: 20rpx;
}

.setting-font{
  font-size: 28rpx;
  font-weight: 400;
  color:#333333;
  margin-left: 30rpx;
  margin-top: 5rpx;
  align-items: center;
  font-size: 20px;
}

